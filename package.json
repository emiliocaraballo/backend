{
  "name": "backend-ts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev": "ts-node-dev -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "start:prod": "NODE_ENV=production PORT=3000 node dist/index.js",
    "start:strategy": "NODE_ENV=strategy PORT=3000 node dist/index.js",
    "test": "npm run build && mocha dist/tests --exit",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create -n",
    "migration:run": "npm run build && ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "npm run build && ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert",
    "seed:config": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed",
    "seed:generate": "ts-node -r tsconfig-paths/register typeorm migration:generate -c seed -n "
  },
  "keywords": [],
  "author": "Emilio Caraballo",
  "license": "ISC",
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/file-type": "^10.9.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.4",
    "@types/pg": "^8.6.1",
    "@types/qrcode": "^1.4.1",
    "@types/sharp": "^0.29.2",
    "@types/speakeasy": "^2.0.6",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/ua-parser-js": "^0.7.36",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "file-type": "^16.5.3",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "kernell-smartinfo": "git@github.com:emiliocaraballo/kernell-smartinfo.git",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
